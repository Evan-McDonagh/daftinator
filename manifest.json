{
  "name": "daftinator",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Injecting stuff...",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Inject!",
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ],
  "icons": {
    "128": "images/home-solid.png"
  }
}